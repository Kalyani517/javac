    }                                    

    private void bequalActionPerformed(java.awt.event.ActionEvent evt) {                                       

        
num2 = Double.parseDouble(T1.getText());
        double ans=0;
        
        if( operator.equals("+"))
        {
            ans=num1+ num2;
        }
        if(operator.equals("-"))
        {
            ans = num1+ num2;
        }
      if(operator.equals("*"))
        {
            ans = num1*num2;
        }
        if(operator.equals("/"))
        {
            ans = num1/num2;
        }
T1.setText(""+ans);
  
   operator=null;
    }                                      

